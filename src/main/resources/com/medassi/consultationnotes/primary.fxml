<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" prefHeight="432.0" prefWidth="416.0" spacing="20.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.medassi.consultationnotes.PrimaryController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <TitledPane animated="false" text="Général">
         <content>
            <GridPane hgap="5.0" prefHeight="89.0" prefWidth="391.0" style="-fx-background-color: #B0F9A7;" vgap="5.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="177.0" minWidth="10.0" prefWidth="88.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="250.0" minWidth="10.0" prefWidth="171.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="250.0" minWidth="10.0" prefWidth="102.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label text="Moyenne générale">
                     <font>
                        <Font size="10.0" />
                     </font></Label>
                  <Label text="Mini de la classe" GridPane.rowIndex="1">
                     <font>
                        <Font size="10.0" />
                     </font></Label>
                  <Label text="Maxi de la classe" GridPane.rowIndex="2">
                     <font>
                        <Font size="10.0" />
                     </font></Label>
                  <Label fx:id="labelGeneMoyenne" prefHeight="17.0" prefWidth="233.0" text="labelGeneMoyenne" GridPane.columnIndex="1" />
                  <Label fx:id="labelGeneMiniEtud" prefHeight="17.0" prefWidth="162.0" text="labelGeneMiniEtud" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <Label fx:id="labelGeneMaxiEtud" prefHeight="17.0" prefWidth="239.0" text="labelGeneMaxiEtud" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <Label fx:id="labelGeneMiniNote" prefHeight="17.0" prefWidth="90.0" text="labelGeneMiniNote" GridPane.columnIndex="2" GridPane.rowIndex="1">
                     <font>
                        <Font size="10.0" />
                     </font></Label>
                  <Label fx:id="labelGeneMaxiNote" prefHeight="17.0" prefWidth="92.0" text="labelGeneMaxiNote" GridPane.columnIndex="2" GridPane.rowIndex="2">
                     <font>
                        <Font size="10.0" />
                     </font></Label>
               </children>
            </GridPane>
         </content>
      </TitledPane>
      <TitledPane animated="false" text="Etudiant">
         <content>
            <HBox prefHeight="26.0" prefWidth="391.0" spacing="10.0" style="-fx-background-color: blue;">
               <children>
                  <ComboBox fx:id="cbEtudiants" prefWidth="150.0" />
                  <Button mnemonicParsing="false" onAction="#onActionRechercherEtudiant" text="Rechercher" />
               </children>
            </HBox>
         </content>
      </TitledPane>
      <TitledPane animated="false" text="Matière">
         <content>
            <HBox prefHeight="4.0" prefWidth="391.0" spacing="10.0" style="-fx-background-color: gold;">
               <children>
                  <ComboBox fx:id="cbMatieres" prefWidth="150.0" />
                  <Button mnemonicParsing="false" onAction="#onActionRechercherMatiere" text="Rechercher" />
               </children>
            </HBox>
         </content>
      </TitledPane>
      <TitledPane animated="false" text="Devoir">
         <content>
            <HBox prefHeight="0.0" prefWidth="391.0" spacing="10.0" style="-fx-background-color: red;">
               <children>
                  <ComboBox fx:id="cbDevoirs" prefWidth="150.0" />
                  <Button mnemonicParsing="false" onAction="#onActionRechercherDevoir" text="Rechercher" />
               </children>
            </HBox>
         </content>
      </TitledPane>
   </children>
</VBox>
